#!/usr/bin/env ruby
ENV["RACK_ENV"] = "development"
if ENV["LOG_LEVEL"].to_s == ""
  ENV["LOG_LEVEL"] = "warn"
end
require "pathname"
require "dotenv"
project_root = Pathname(__dir__) / ".."
Dotenv.load(project_root / ".env.development",
            project_root / ".env.development.local")
require_relative project_root / "app" / "bootstrap"
Bootstrap.new.bootstrap!
binding.irb
